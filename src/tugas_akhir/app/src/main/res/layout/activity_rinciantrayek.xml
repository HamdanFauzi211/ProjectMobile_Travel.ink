<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgdetailtrayektravel"
    tools:context=".RinciantrayekActivity">

    <TextView
        android:id="@+id/trayek2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroybold"
        android:text="Trayek"
        android:textColor="#CBCECD"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.572" />

    <TextView
        android:id="@+id/rincian2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroybold"
        android:text="Rincian"
        android:textColor="#F89C38"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.434"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.572" />

    <ImageView
        android:id="@+id/iconmobil"
        android:layout_width="43dp"
        android:layout_height="44dp"
        android:background="@drawable/iconmobil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trayek2"
        app:layout_constraintVertical_bias="0.086" />

    <TextView
        android:id="@+id/rincianmobil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroybold"
        android:text="Rincian Mobil"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toEndOf="@+id/iconmobil"
        app:layout_constraintTop_toBottomOf="@+id/trayek2"
        app:layout_constraintVertical_bias="0.076" />


    <TextView
        android:id="@+id/rincianmobil1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroysemibold"
        android:text="Toyota Hiache Premio 2016"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.117"
        app:layout_constraintStart_toEndOf="@+id/iconmobil"
        app:layout_constraintTop_toBottomOf="@+id/trayek2"
        app:layout_constraintVertical_bias="0.156" />

    <ImageView
        android:id="@+id/iconkursi"
        android:layout_width="43dp"
        android:layout_height="44dp"
        android:background="@drawable/kursi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trayek2"
        app:layout_constraintVertical_bias="0.301" />

    <TextView
        android:id="@+id/kapasitas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:layout_marginRight="240dp"
        android:fontFamily="@font/gilroybold"
        android:text="Kapasitas"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toEndOf="@+id/iconkursi"
        app:layout_constraintTop_toBottomOf="@+id/rincianmobil1"
        app:layout_constraintVertical_bias="0.092" />

    <TextView
        android:id="@+id/kapasitas2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="116dp"
        android:layout_marginRight="116dp"
        android:fontFamily="@font/gilroysemibold"
        android:text="1 Sopir + 9 Penumpang"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.226"
        app:layout_constraintStart_toEndOf="@+id/iconkursi"
        app:layout_constraintTop_toBottomOf="@+id/kapasitas"
        app:layout_constraintVertical_bias="0.039" />

    <ImageView
        android:id="@+id/iconkursi2"
        android:layout_width="43dp"
        android:layout_height="44dp"
        android:background="@drawable/kursi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trayek2"
        app:layout_constraintVertical_bias="0.522" />

    <TextView
        android:id="@+id/sisakursi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroybold"
        android:text="Kursi Kosong"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toEndOf="@+id/iconkursi2"
        app:layout_constraintTop_toBottomOf="@+id/kapasitas2"
        app:layout_constraintVertical_bias="0.125" />

    <TextView
        android:id="@+id/sisakursi2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroysemibold"
        android:text="4 Tersedia"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toEndOf="@+id/iconkursi2"
        app:layout_constraintTop_toBottomOf="@+id/kapasitas2"
        app:layout_constraintVertical_bias="0.258" />

    <ImageButton
        android:layout_width="391dp"
        android:layout_height="48dp"
        android:background="@drawable/buttonpesanrincian"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.98"
        tools:ignore="SpeakableTextPresentCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>